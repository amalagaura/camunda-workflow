# Ignore bundler config.
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/log/*
!/log/.keep
/tmp

# Ignore uploaded files in development
/storage/*

# Ignore coverage reports
/coverage/

#Ignore Documentation
/code/
/.yardoc/
/doc/
/rdoc/

# persists some state between rspec runs
spec/spec_failures.txt
spec/failures.txt

# Ignore windows/mac/linux files
**/.DS_Store?
**/ehthumbs.db
**/Icon?
**/Thumbs.db

# Ignore Submlime Text editor config files.
/*.sublime-project
/*.sublime-workspace

# SFTP Sublimetext plugin config file
**/sftp-config.json

# Ignore precompiled assets
/public/assets

# Ignore auto generated files from rubocop
/rubocop

# ignore the output from the rails-erd gem
erd.pdf

# Ignore the Rubymine config folder
.idea/*

# Docker. Ignore for now.
**/Dockerfile
**/docker-compose.yml

# metric_fu. Ignore for now
**/.metrics

# Ignore .swp file from vim
**/*.swp

# Ignore master key for decrypting credentials and more.
/config/master.key

**/.bashrc

.bashrc
.byebug_history
rails_best_practices_output.html

/public/uploads
/public/share
/node_modules
